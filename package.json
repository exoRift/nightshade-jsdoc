{
  "name": "nightshade-jsdoc",
  "version": "1.0.0",
  "description": "A JSDoc template dedicated to a snappy, sleek, reactive, and smooth design based in React",
  "main": "publish.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "export": "jsdoc -r -c test/docs.json",
    "test": "npm run export && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exoRift/nightshade-jsdoc.git"
  },
  "keywords": [
    "react",
    "jsdoc",
    "template",
    "website",
    "docsite",
    "nodejs"
  ],
  "author": "exoRift",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exoRift/nightshade-jsdoc/issues"
  },
  "homepage": "https://github.com/exoRift/nightshade-jsdoc#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ejs": "^3.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.4"
  }
}
